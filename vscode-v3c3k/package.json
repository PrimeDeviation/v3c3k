{
  "name": "vscode-v3c3k",
  "displayName": "V3C3K",
  "description": "VSCode extension for V3C3K - AI-assisted Product Validation Tool",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:v3c3k.checkCLI"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "v3c3k.checkCLI",
        "title": "V3C3K: Check CLI Installation"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.0.0"
  }
}
